{
    "deno.enable": true,
    "deno.path": "/opt/homebrew/bin//deno",
    "editor.tokenColorCustomizations": {
        "comments": "rgb(213, 202, 133)",
        "strings": "rgb(174, 139, 138)",
        "types": "rgb(147, 77, 87)",
        "keywords": "rgb(201, 106, 172)",
        "variables": "rgb(176, 193, 187)",
        "functions": "rgb(117, 160, 171)",
        "numbers": "rgb(64, 179, 206)",
        "textMateRules": [
            {
                "name": "keyword.operator",
                "scope": [
                    "keyword.operator"
                ],
                "settings": {
                    "foreground": "rgb(176, 193, 187)"
                }
                },
                {
                "name": "keyword.control",
                "scope": [
                    "keyword.control"
                ],
                "settings": {
                    "foreground": "rgb(147, 77, 87)"
                }
            }
          ]
    }
